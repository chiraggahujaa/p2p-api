# Server Configuration
PORT=5000
NODE_ENV=development
API_BASE_URL=your_api_base_url

# Database Configuration (Supabase)
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
DB_PASSWORD=your_database_password

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# CORS Configuration
FRONTEND_URL=your_frontend_url
ALLOWED_ORIGINS=allowed_origins

# Maintance Mode
MAINTENANCE_MODE=false

# External API's
OPENWEATHER_BASE_URL=your_openweather_api_key

# Sandbox.co.in DigiLocker KYC API Configuration
SANDBOX_API_URL=https://api.sandbox.co.in
SANDBOX_API_KEY=your_sandbox_api_key
# SANDBOX_CLIENT_ID=your_client_id_here  # Remove or leave empty until you get DigiLocker partner credentials
SANDBOX_CLIENT_SECRET=your_sandbox_client_secret
SANDBOX_API_VERSION=2.0

# DigiLocker Configuration
DIGILOCKER_REDIRECT_URL=your_digilocker_redirect_url
DIGILOCKER_SESSION_TIMEOUT=3600

# Railway Environment Variables (for local development reference)
RAILWAY_PUBLIC_DOMAIN=p2p-api-production.up.railway.app
RAILWAY_PRIVATE_DOMAIN=p2p-api.railway.internal
RAILWAY_PROJECT_NAME=p2p-api
RAILWAY_ENVIRONMENT_NAME=production
RAILWAY_SERVICE_NAME=p2p-api
RAILWAY_PROJECT_ID=your_railway_project_id
RAILWAY_ENVIRONMENT_ID=your_railway_environment_id
RAILWAY_SERVICE_ID=your_railway_service_id

# Test Configuration
TEST_USER_EMAIL=your_test_user_email
TEST_USER_PASSWORD=your_test_user_password