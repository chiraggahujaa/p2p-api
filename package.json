{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development nodemon",
    "build": "tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "echo 'No linter configured - skipping'",
    "typecheck": "tsc --noEmit",
    "postinstall": "if [ -d .git ]; then lefthook install; fi",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:link": "supabase link",
    "supabase:push": "supabase db push",
    "supabase:reset": "supabase db reset",
    "supabase:pull": "supabase db pull",
    "supabase:diff": "supabase db diff",
    "supabase:generate": "supabase gen types typescript --local > src/types/supabase.ts",
    "supabase:generate:remote": "supabase gen types typescript --project-id=$SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "db:push": "npm run supabase:push",
    "db:reset": "npm run supabase:reset",
    "db:migrate": "npm run supabase:push",
    "test": "cypress run",
    "test:open": "cypress open",
    
    "test:auth": "cypress run --spec 'cypress/e2e/api/auth/**/*.cy.ts'",
    "test:users": "cypress run --spec 'cypress/e2e/api/users/**/*.cy.ts'",
    "test:items": "cypress run --spec 'cypress/e2e/api/items/**/*.cy.ts'",
    "test:bookings": "cypress run --spec 'cypress/e2e/api/bookings/**/*.cy.ts'",
    "test:categories": "cypress run --spec 'cypress/e2e/api/categories/**/*.cy.ts'",
    "test:addresses": "cypress run --spec 'cypress/e2e/api/addresses/**/*.cy.ts'",
    "test:files": "cypress run --spec 'cypress/e2e/api/files/**/*.cy.ts'",
    
    "test:integration": "cypress run --spec 'cypress/e2e/integration/**/*.cy.ts'",
    "test:smoke": "cypress run --spec 'cypress/e2e/smoke/**/*.cy.ts'",
    "test:regression": "cypress run --spec 'cypress/e2e/regression/**/*.cy.ts'",
    
    "test:performance": "cross-env PERFORMANCE_TESTING=true cypress run --spec 'cypress/e2e/api/**/*.cy.ts'",
    "test:load": "cross-env LOAD_TESTING=true cypress run --spec 'cypress/e2e/api/**/*.cy.ts'",
    
    "test:dev": "cross-env NODE_ENV=development cypress run",
    "test:staging": "cross-env NODE_ENV=staging cypress run",
    "test:prod": "cross-env NODE_ENV=production cypress run --spec 'cypress/e2e/smoke/**/*.cy.ts'",
    
    "test:headless": "cypress run --headless",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:edge": "cypress run --browser edge",
    
    "test:parallel": "cross-env PARALLEL=true cypress run --parallel --record --key=$CYPRESS_RECORD_KEY",
    "test:record": "cypress run --record --key=$CYPRESS_RECORD_KEY",
    
    "test:ci": "cross-env NODE_ENV=test cypress run --headless --browser chrome --config video=false,screenshotOnRunFailure=true",
    "test:ci:parallel": "cross-env NODE_ENV=test PARALLEL=true cypress run --parallel --record --key=$CYPRESS_RECORD_KEY --headless --browser chrome",
    "test:ci:smoke": "cross-env NODE_ENV=test cypress run --spec 'cypress/e2e/smoke/**/*.cy.ts' --headless --browser chrome --config video=false",
    
    "test:tags": "cypress run --env TEST_TAGS=$TEST_TAGS",
    "test:fast": "cross-env FAIL_FAST=true cypress run",
    
    "test:seed": "echo 'Seeding test database...' && cypress run --spec 'cypress/support/commands/database-commands.ts'",
    "test:clean": "echo 'Cleaning test database...' && cypress run --spec 'cypress/support/commands/database-commands.ts'",
    
    "pretest": "echo 'Starting comprehensive API tests...'",
    "posttest": "echo 'API tests completed'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@cypress/grep": "^4.1.1",
    "@evilmartians/lefthook": "^1.12.3",
    "@faker-js/faker": "^10.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "cross-env": "^10.0.0",
    "cypress": "^15.0.0",
    "cypress-plugin-api": "^2.11.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
