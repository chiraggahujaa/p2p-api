{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "echo 'No linter configured - skipping'",
    "typecheck": "tsc --noEmit",
    "postinstall": "if [ -d .git ]; then lefthook install; fi",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:link": "supabase link",
    "supabase:push": "supabase db push",
    "supabase:reset": "supabase db reset",
    "supabase:pull": "supabase db pull",
    "supabase:diff": "supabase db diff",
    "supabase:generate": "supabase gen types typescript --local > src/types/supabase.ts",
    "supabase:generate:remote": "supabase gen types typescript --project-id=$SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "db:push": "npm run supabase:push",
    "db:reset": "npm run supabase:reset",
    "db:migrate": "npm run supabase:push",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:auth": "cypress run --spec 'cypress/e2e-tests/auth/**/*.cy.ts'",
    "test:users": "cypress run --spec 'cypress/e2e-tests/users/**/*.cy.ts'",
    "test:items": "cypress run --spec 'cypress/e2e-tests/items/**/*.cy.ts'",
    "test:bookings": "cypress run --spec 'cypress/e2e-tests/bookings/**/*.cy.ts'",
    "test:categories": "cypress run --spec 'cypress/e2e-tests/categories/**/*.cy.ts'",
    "test:integration": "cypress run --spec 'cypress/e2e-tests/integration/**/*.cy.ts'",
    "test:basic": "cypress run --spec 'cypress/e2e-tests/**/*-basic.cy.ts'",
    "test:advanced": "cypress run --spec 'cypress/e2e-tests/**/*-advanced.cy.ts'",
    "test:headless": "cypress run --headless",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:record": "cypress run --record --key=$CYPRESS_RECORD_KEY",
    "test:parallel": "cypress run --parallel --record --key=$CYPRESS_RECORD_KEY",
    "test:ci": "cross-env NODE_ENV=test cypress run --headless --browser chrome --config video=false",
    "test:smoke": "cypress run --spec 'cypress/e2e-tests/integration/api-integration.cy.ts' --config video=false",
    "pretest": "echo 'Starting API tests...'",
    "posttest": "echo 'API tests completed'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "api": "^6.1.3",
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@cypress/grep": "^4.1.1",
    "@evilmartians/lefthook": "^1.12.3",
    "@faker-js/faker": "^10.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "cross-env": "^10.0.0",
    "cypress": "^15.0.0",
    "cypress-plugin-api": "^2.11.2",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
